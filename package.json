{
  "name": "project-1-setup",
  "version": "1.0.0",
  "description": "WDI setup for project 1",
  "main": "gulpfile.js",
  "author": "Daniel IZQUIERDO <danielizquierdofernandez89@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "gulp deploy"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gulp": {
    "srcDir": "src/",
    "destDir": "public/",
    "src": {
      "js": "src/js/",
      "scss": "src/scss/",
      "images": "src/images/",
      "fonts": "src/fonts/",
      "audio": "src/audio/"
    },
    "dest": {
      "js": "public/js/",
      "css": "public/css/",
      "images": "public/images/",
      "fonts": "public/fonts/",
      "audio": "public/audio/"
    },
    "main": {
      "js": "app.js",
      "index": "index.html",
      "scss": "style.scss",
      "server": "index.js",
      "gulpfile": "gulpfile.js",
      "packageJson": "package.json"
    },
    "output": {
      "js": "app.js",
      "css": "style.css"
    },
    "selectors": {
      "all": "*",
      "js": "**/*.js",
      "css": "**/*.css",
      "scss": "**/*.scss",
      "html": "**/*.html",
      "images": "**/*.{png,jpg,jpeg,gif,ico}",
      "fonts": "**/*.{woff,woff2,otf,eot,svg,ttf}",
      "audio": "**/*.{mp3,wav,webm,ogg}"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bower": "^1.8.14",
    "browser-sync": "^2.29.3",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-htmlhint": "^0.3.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject": "^5.0.5",
    "gulp-jshint": "^2.1.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-notify": "^3.2.0",
    "gulp-order": "^1.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "htmlhint-stylish": "^1.0.3",
    "jshint": "^2.13.6",
    "jshint-stylish": "^2.2.1",
    "node-sass": "^4.14.1",
    "require-dir": "^1.2.0"
  },
  "dependencies": {}
}
